DROP TABLE IF EXISTS `wp_crx1wxh07f_spiderfacebook_login`;

# Table structure of table `wp_crx1wxh07f_spiderfacebook_login`

CREATE TABLE `wp_crx1wxh07f_spiderfacebook_login` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` varchar(200) NOT NULL,
  `username` varchar(200) NOT NULL,
  `password` varchar(200) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ;

