# Table: `wp_crx1wxh07f_postmeta`
# Approximate rows expected in table: 1156

# Delete any existing table `wp_crx1wxh07f_postmeta`

DROP TABLE IF EXISTS `wp_crx1wxh07f_postmeta`;

# Table structure of table `wp_crx1wxh07f_postmeta`

CREATE TABLE `wp_crx1wxh07f_postmeta` (
  `meta_id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `post_id` bigint(20) unsigned NOT NULL DEFAULT '0',
  `meta_key` varchar(255) DEFAULT NULL,
  `meta_value` longtext,
  PRIMARY KEY (`meta_id`),
  KEY `post_id` (`post_id`),
  KEY `meta_key` (`meta_key`)
) ENGINE=InnoDB AUTO_INCREMENT=2972 DEFAULT CHARSET=utf8 ;

