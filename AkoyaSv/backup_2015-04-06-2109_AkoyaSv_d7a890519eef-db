# Table: `wp_icl_languages_translations`
# Approximate rows expected in table: 3557

# Delete any existing table `wp_icl_languages_translations`

DROP TABLE IF EXISTS `wp_icl_languages_translations`;

# Table structure of table `wp_icl_languages_translations`

CREATE TABLE `wp_icl_languages_translations` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `language_code` varchar(7) NOT NULL,
  `display_language_code` varchar(7) NOT NULL,
  `name` varchar(255) NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `language_code` (`language_code`,`display_language_code`)
) ENGINE=InnoDB AUTO_INCREMENT=3558 DEFAULT CHARSET=utf8 ;

